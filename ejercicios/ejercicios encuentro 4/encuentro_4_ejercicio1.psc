Algoritmo sin_titulo
	Definir sueldomini, sueldo Como Entero;
	Escribir " ingrese el valor de su sueldo";
	leer sueldo;
	
	Escribir "ingrese el valor del sueldo minimo";
	leer sueldomini;
	Si sueldo > sueldomini Entonces
		escribir "su sueldo es mayor al sueldo minimo"
	FinSi
	si sueldo <= sueldomini Entonces
		Escribir "su sueldo es el minimo"
	FinSi
FinAlgoritmo
